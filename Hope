#include <iostream>
#include <string>
using namespace std;
int str_count(string x, string y){
  int c=0;
  int cc=0;
  int co=0;
   for(int i=0; i<x.length(); i++){
       c+= (int)x[i];
   }
   for(int j=0; j<(y.length()+1)-x.length(); j++){
      for(int i=0; i<x.length(); i++){
       cc+=(int)y[i+j];
   }
   if(c==cc){
      co++;
      cc=0;
   }
   }
   return co;
}

int main(){
    
int g= str_count("Kar", "Kar Hatem is Kar");

cout<<g;

return 0;   
}

Get-PnpDevice -PresentOnly | Where-Object { $_.Class -eq "Ports" } | ForEach-Object { $_.DeviceID, $_.FriendlyName }

wmic path Win32_PnPEntity where "Name like '%USB%'" get Caption, Description, PNPDeviceID

Get-ItemProperty -Path HKLM:\SYSTEM\CurrentControlSet\Enum\USB | Select-Object DeviceID, FriendlyName
